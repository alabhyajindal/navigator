<nav>
  <p>
    Navigator
  </p>
  <% if current_user %>
    <div>
      <p><%= link_to 'Edit Account', edit_user_path(current_user) %></p>
      <p><%= link_to 'Sign out', session_path, data: { turbo_method: :delete } %></p>
    </div>
  <% else %>
    <div>
      <p><%= link_to 'Sign in', signin_path %></p>
      <p><%= link_to 'Sign up', signup_path %></p>
    </div>
  <% end %>
</nav>