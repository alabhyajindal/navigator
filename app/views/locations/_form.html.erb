<%= form_with model: location do |f| %>
  <%= f.label :city %>
  <%= f.text_field :city, autocomplete: "off" %>

  <%= f.label :date_visited %>
  <%= f.date_select :date_visited, order: [:day, :month, :year], start_year: 1900, end_year: Date.today.year %>

  <%= f.label :date_left %>
  <%= f.date_select :date_left, order: [:day, :month, :year], start_year: 1900, end_year: Date.today.year %>

  <%= f.submit %>
<% end %>