<nav>
  <ul>
    <% if current_user %>
      <li><%= link_to 'Edit Account', edit_user_path(current_user) %></li>
      <li><%= link_to 'Sign Out', session_path, data: { turbo_method: :delete } %></li>
    <% else %>
      <li><%= link_to 'Sign In', signin_path %></li>
      <li><%= link_to 'Sign Up', signup_path %></li>
    <% end %>
  </ul>
</nav>