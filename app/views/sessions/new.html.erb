<h1>Sign in</h1>
<p>Don't have an account? <%= link_to 'Sign up', signup_path %></p>

<%= form_with url: session_path do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email, autofocus: true %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit 'Sign in' %>
<% end %>